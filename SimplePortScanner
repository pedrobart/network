
'''
simple portscanner
'''




import socket

sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)

host = ""
port = 

def portscanner(port):
    if sock.connect_ex((host,port)):
        print("port %d is closed" % (port))
    else:
        print("port %d is opened" % (port))
        
portscanner(port)

